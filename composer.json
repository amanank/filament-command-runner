{
    "name": "amanank/filament-command-runner",
    "description": "A reusable Composer package that provides a secure, extensible Filament UI for running predefined Artisan commands.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Amanank",
            "email": "info@amanank.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "filament/filament": "^3.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.0",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "EeqanLtd\\FilamentCommandRunner\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EeqanLtd\\FilamentCommandRunner\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EeqanLtd\\FilamentCommandRunner\\Providers\\FilamentCommandRunnerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
